{
    "name": "{{ forumVersion }}",
    "config": {
        "main": "net.atos.entng.forum.Forum",
        "port": 8024,
        {% if csrfToken %}"csrf-token": true,{% endif %}
        "app-name": "Forum",
        "app-address": "/forum",
        "app-icon": "forum-large",
        "app-type" : "END_USER",
        "host": "https://{{ host }}",
        "ssl": true,
        {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
        "auto-redeploy": false,
        "userbook-host": "https://{{ host }}",
        "integration-mode": "HTTP",
        "app-registry.port": 8012,
        "mode": "prod",
        "entcore.port": 8009,
        "publicConf": {
            "xiti": {
                "ID_SERVICE": {
                    "default": 2
                },
                "LIBELLE_SERVICE": {
                    "default": "PRODUCTION_COLLABORATIVE"
                }
            }
        }
    }
}